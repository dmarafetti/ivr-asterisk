<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<configuration>
    
    <include resource="org/springframework/boot/logging/logback/base.xml"/>
    
    <root level="INFO">
        <appender-ref ref="CONSOLE" />
        <appender-ref ref="FILE" />
    </root>
    
    <logger name="org.springframework" level="OFF"/>
    
</configuration>
